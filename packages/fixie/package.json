    "test": "yarn node --experimental-vm-modules $(yarn bin jest) --clearCache && yarn node --experimental-vm-modules $(yarn bin jest) --verbose",
    "lint": "eslint .",
    "prepack": "yarn build"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "main": "dist/src/index.js",
  "bin": "dist/src/cli.js",
  "types": "dist/src/index.d.ts",
  "dependencies": {
    "@fixieai/fixie-common": "^1.0.14",
    "axios": "^1.6.3",
    "commander": "^11.0.0",
    "execa": "^8.0.1",
    "extract-files": "^13.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "open": "^9.1.0",
    "ora": "^7.0.1",
    "terminal-kit": "^3.0.0",
    "type-fest": "^4.3.1",
    "typescript-json-schema": "^0.61.0",
    "untildify": "^5.0.0",
    "watcher": "^2.3.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/extract-files": "^8.1.1",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.4.1",
