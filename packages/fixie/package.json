  "license": "MIT",
  "repository": "fixie-ai/fixie-sdk",
  "bugs": "https://github.com/fixie-ai/fixie-sdk/issues",
  "homepage": "https://fixie.ai",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --no-warnings dist/src/cli.js",
